{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"projection": {
			"type": "string"
		},
		"layers": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"parameters": {
						"type": "array",
						"items": {
							"type": "object",
							"properties": {
								"key": {
									"type": "string"
								},
								"value": {
									"type": "string"
								}
							},
							"required": [
								"key",
								"value"
							]
						}
					}
				},
				"required": [
					"name",
					"parameters"
				]
			}
		}
	},
	"required": [
		"name",
		"projection",
		"layers"
	]
}